<touchdelegates>
    <touchdelegate name="onclick"/>
</touchdelegates>
<layout width="100" height="125" type="relative" touch="true" touchdelegate="onclick" cascade_opacity="true" rotation_around_scale="1.1">
    <img atlas="atlas/xocdia.plist" frame="border_white_32x32.png" width="match_parent" height="match_parent" opacity="200" color="#0f0f0f" cap_top="8" cap_left="8" cap_right="8" cap_bottom="8" align="center" cascade_opacity="true"/>
