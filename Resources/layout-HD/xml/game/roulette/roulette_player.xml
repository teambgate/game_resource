    <label width="match_parent" font="atlas/xocdiafont.fnt" font_atlas="atlas/xocdia.plist" font_frame="font30.png" size="30" align="top_center_horizontal" hashkey="name" type="nowrap" max_character="7" cascade_opacity="true" margin_top="-7" scale="0.533333">tukiki...</label>
    <label width="match_parent" font="atlas/xocdiafont.fnt" font_atlas="atlas/xocdia.plist" font_frame="font30.png" size="30" align="bottom_center_horizontal" color="yellow" hashkey="money" type="nowrap" cascade_opacity="true" margin_bottom="-6" scale="0.6666667">200</label>
	 <img atlas="atlas/xocdia.plist" frame="home.png" align="left_bottom" width="wrap_content" height="wrap_content" margin_bottom="20" hashkey="host_flag" visible="false" scale="1.5" cascade_opacity="true"/>
