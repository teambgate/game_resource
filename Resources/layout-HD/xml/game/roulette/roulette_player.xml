        <img name="avatar" atlas="atlas/xocdia.plist" frame="avatar_default.png" preserve_atlas="atlas/xocdia.plist" preserve_frame="avatar_1.png" align="center" width="fill_parent" height="fill_parent" hashkey="avatar" cascade_opacity="true"/>
