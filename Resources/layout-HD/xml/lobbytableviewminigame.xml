							<scaleto time="0.1" x="0.95" y="0.95"/>
						</action>
					</touchmove>	
					<touchend>
						<action id="this">
							<scaleto time="0.1" x="1" y="1"/>
						</action>
					</touchend>
					<touchcancel>
						<action id="this">
							<scaleto time="0.1" x="1" y="1"/>
						</action>
					</touchcancel>
				</layout>
				<layout width="173" height="96" align="left_center_vertical" margin_left="415" type="relative" cascade_opacity="true" hashkey="item_3" touchdelegate="touch_item" touch="true" visible="false" opacity="0">
					<img atlas="atlas/choice_room_2.plist" frame="tbcoin.png" align="center" visible="false" hashkey="tbcoin_3"/>
					<img atlas="atlas/choice_room_2.plist" frame="tbchip.png" align="center" visible="false" hashkey="tbchip_3"/>
					<img atlas="atlas/choice_room_2.plist" frame="tbchip-gray.png" atlas_disable="atlas/choice_room_2.plist" frame_disable_coin="tbcoin-gray.png" align="center" visible="false" hashkey="img_disable_3"/>
					<label font="fonts/font16.fnt" scale="1.25" color="#FECD0B" align="top_center_horizontal" margin_left="10" margin_top="37" type="nowrap" hashkey="title_chip_3" cascade_opacity="true">1-100</label>
					<label font="fonts/font16.fnt" scale="1.25" color="YELLOW" align="top_center_horizontal" margin_left="10" margin_top="37" type="nowrap" hashkey="title_coin_3" cascade_opacity="true">1-100</label>
					<touchbegan>
						<action id="this">
							<scaleto time="0.1" x="0.95" y="0.95"/>
						</action>
					</touchbegan>
					<touchmove>
						<action id="this">
							<scaleto time="0.1" x="0.95" y="0.95"/>
						</action>
					</touchmove>	
					<touchend>
						<action id="this">
							<scaleto time="0.1" x="1" y="1"/>
						</action>
					</touchend>
					<touchcancel>
						<action id="this">
							<scaleto time="0.1" x="1" y="1"/>
						</action>
					</touchcancel>
				</layout>
				<layout width="173" height="96" align="left_center_vertical" margin_left="615" type="relative" cascade_opacity="true" hashkey="item_4" touchdelegate="touch_item" touch="true" visible="false" opacity="0">
					<img atlas="atlas/choice_room_2.plist" frame="tbcoin.png" align="center" visible="false" hashkey="tbcoin_4"/>
					<img atlas="atlas/choice_room_2.plist" frame="tbchip.png" align="center" visible="false" hashkey="tbchip_4"/>
					<img atlas="atlas/choice_room_2.plist" frame="tbchip-gray.png" atlas_disable="atlas/choice_room_2.plist" frame_disable_coin="tbcoin-gray.png" align="center" visible="false" hashkey="img_disable_4"/>
					<label font="fonts/font16.fnt" scale="1.25" color="#FECD0B" align="top_center_horizontal" margin_left="10" margin_top="37" type="nowrap" hashkey="title_chip_4" cascade_opacity="true">1-100</label>
					<label font="fonts/font16.fnt" scale="1.25" color="YELLOW" align="top_center_horizontal" margin_left="10" margin_top="37" type="nowrap" hashkey="title_coin_4" cascade_opacity="true">1-100</label>
					<touchbegan>
						<action id="this">
							<scaleto time="0.1" x="0.95" y="0.95"/>
						</action>
					</touchbegan>
					<touchmove>
						<action id="this">
							<scaleto time="0.1" x="0.95" y="0.95"/>
						</action>
					</touchmove>	
					<touchend>
						<action id="this">
							<scaleto time="0.1" x="1" y="1"/>
