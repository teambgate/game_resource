<touchdelegates>
    <touchdelegate name="onclick"/>
</touchdelegates>
<layout type="relative" touchdelegate="onclick" cascade_opacity="true">
    <img name="image1" atlas="atlas/user_info.plist" frame="tab_thongke_1.png" hashkey="image1" width="match_parent" height="match_parent" align="center" visible="true"/>
    <img name="image2" atlas="atlas/user_info.plist" frame="tab_thongke_2.png" hashkey="image2" width="match_parent" height="match_parent" align="center" visible="false"/>
    <touchbegan>
        <action id="this">
            <fadeto time="0.1" to="150"/>
        </action>
    </touchbegan>
    <touchend>
        <action id="this">
             <fadeto time="0.1" to="255"/>
        </action>
    </touchend>
    <touchcancel>
        <action id="this">
             <fadeto time="0.1" to="255"/>
        </action>
    </touchcancel>
</layout>
