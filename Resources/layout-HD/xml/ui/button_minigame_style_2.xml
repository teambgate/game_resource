<touchdelegates>
    <touchdelegate name="onclick"/>
</touchdelegates>
<layout width="wrap_content" height="wrap_content" touchdelegate="onclick">
	<img name="image" atlas="atlas/lobby.plist" frame="button_register.png" hashkey="image" align="center" anchor-y="0"/>
	<touchbegan>
		<action id="this:image">
			<scaleto time="0.1" x="0.9" y="0.9"/>
		</action>
	</touchbegan>	
	<touchend>
		<action id="this:image">
			<scaleto time="0.25" x="1" y="1"/>
		</action>
	</touchend>
	<touchcancel>
		<action id="this:image">
			<scaleto time="0.25" x="1" y="1"/>
		</action>
	</touchcancel>
